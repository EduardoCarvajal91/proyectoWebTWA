{% extends 'base.html.twig' %}

{# app/Resources/views/default/mis_proyectos.html.twig #}

{% block body %}
    <div>
    <ul>
    {% for proyecto in proyectos %}
        <li>
        <a href="{{ path('ver_proyecto', {'nombre': proyecto.nombre}) }}">
            {{ proyecto.nombre }}
        </a>
        </li>
    {% endfor %}
    </ul>
    </div>

{% endblock %}